        },
        "shop_fees": {
          "name": "shop_fees",
          "type": "real",
          "primaryKey": false,
          "notNull": false,
          "autoincrement": false
        },
        "total": {
          "name": "total",
          "type": "real",
          "primaryKey": false,
          "notNull": false,
          "autoincrement": false
        },
        "cost": {
          "name": "cost",
          "type": "real",
          "primaryKey": false,
          "notNull": false,
          "autoincrement": false
        },
        "parts_margin": {
          "name": "parts_margin",
          "type": "real",
          "primaryKey": false,
          "notNull": false,
          "autoincrement": false
        },
        "margin": {
          "name": "margin",
          "type": "real",
          "primaryKey": false,
          "notNull": false,
          "autoincrement": false
        }
      },
      "name": "estimate_totals",
      "isExisting": false,
      "definition": "\n  select date,id,coalesce(l.labor,0) as labor,\n    coalesce(p.parts,0) as parts, \n    coalesce(o.oil,0) as oil,\n    coalesce(l.labor,0)+coalesce(p.parts,0)+coalesce(o.oil,0) as subtotal,\n    round(0.081*(coalesce(p.parts,0)+coalesce(o.oil,0)),2) as tax,\n    CASE WHEN coalesce(l.labor,0)+coalesce(p.parts,0)+coalesce(o.oil,0) >500 THEN 15\n    ELSE round(0.03*(coalesce(l.labor,0)+coalesce(p.parts,0)+coalesce(o.oil,0)),2) END as shop_fees,\n    coalesce(l.labor,0)+coalesce(p.parts,0)+coalesce(o.oil,0)+\n    round(0.081*(coalesce(p.parts,0)+coalesce(o.oil,0)),2)+\n    CASE WHEN coalesce(l.labor,0)+coalesce(p.parts,0)+coalesce(o.oil,0) >500 THEN 15\n    ELSE round(0.03*(coalesce(l.labor,0)+coalesce(p.parts,0)+coalesce(o.oil,0)),2) END as total,\n    coalesce(p.p_cost,0) + coalesce(o.o_cost,0) as cost,\n    coalesce(p.parts,0) - coalesce(p.p_cost,0) as parts_margin,\n    coalesce(l.labor,0)+coalesce(p.parts,0)+coalesce(o.oil,0)+\n    round(0.081*(coalesce(l.labor,0)+coalesce(p.parts,0)+coalesce(o.oil,0)),2)+\n    CASE WHEN coalesce(l.labor,0)+coalesce(p.parts,0)+coalesce(o.oil,0) >500 THEN 15\n    ELSE round(0.03*(coalesce(l.labor,0)+coalesce(p.parts,0)+coalesce(o.oil,0)),2) END -\n    coalesce(p.p_cost,0) + coalesce(o.o_cost,0) as margin \nfrom estimates r\nleft join (\n    select estimate_id,coalesce(sum(price),0) as labor\n            from labor\n        group by estimate_id) l\non l.estimate_id=r.id\nleft join (\n    select estimate_id,coalesce(sum(unit_price*quantity),0) as parts,\n    coalesce(sum(cost*quantity),0) as p_cost\n            from parts \n        group by estimate_id) p \non p.estimate_id=r.id \nleft join (\n    select estimate_id,coalesce(sum(price_per_unit*quantity),0) as oil,\n    coalesce(sum(cost*quantity),0) as o_cost\n            from oil \n        group by estimate_id) o \non o.estimate_id=r.id;\n  "
    }
  },
